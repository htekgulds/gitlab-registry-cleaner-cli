{
  "name": "gitlab-registry-cleaner-cli",
  "version": "0.3.0",
  "description": "Gitlab Registry Cleaner CLI",
  "author": "Hasan Tekg√ºl <htekgulds@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/htekgulds/gitlab-registry-cleaner-cli.git"
  },
  "keywords": [
    "nodejs",
    "docker",
    "cli",
    "registry",
    "gitlab",
    "cleaner",
    "pkg",
    "esbuild",
    "yargs"
  ],
  "bin": {
    "grc": "dist/index.js"
  },
  "scripts": {
    "build": "esbuild bin/grc.js --platform=node --bundle --outfile=dist/index.js --minify",
    "watch": "npm run build -- --watch",
    "package": "pkg dist/index.js --out-path pkg -d --targets node18-linux-x64,node18-macos-x64,node18-win-x64",
    "lint": "standard"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "axios": "^1.6.2",
    "boxen": "^7.1.1",
    "chalk": "^5.3.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "esbuild": "^0.19.7",
    "pkg": "^5.8.1",
    "standard": "^17.1.0"
  }
}
